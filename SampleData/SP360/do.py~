#!/usr/bin/python

import sys
import subprocess

for i in range(1, len(sys.argv)):
    f = sys.argv[i]
    parts = f.split(".eps")
    subprocess.call("eps2eps "+f+" tmp.eps", shell=True)
    subprocess.call("convert -density 600 -negate -contrast -modulate 110 tmp.eps " + parts[0]+".png", shell=True)
    subprocess.call("rm tmp.eps", shell=True)
    
